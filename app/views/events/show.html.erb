<div class="page-header">
  <h1>Event <%= @event.hashtag %></h1>

</div>
<div class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Username</th>
        <th>Image</th>
        <th>Filter</th>
      </tr>
    </thead>
    <% @photos.each do |photo| %>
      <tbody>
        <tr>
          <td><%= photo.username %></td>
          <td><%= image_tag(photo.url) %></td>
          <td><%= photo.parameters["filter"] %></td>
        </tr>
      </tbody>
    <% end %>
  </table>
</div>
